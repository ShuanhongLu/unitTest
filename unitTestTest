package testCase;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

import testUnit.testUnit;

class testUnitTest {

	@Test
	void testMaxAddArr() {
		int Arr1[]= {-2,11,-4,13,-5,-2};
		assertEquals(20,new testUnit().MaxAddArr(Arr1));
		int Arr2[]= {-1,-3};
		assertEquals(0,new testUnit().MaxAddArr(Arr2));
		int Arr3[]= {-1,-2,0,-3};
		assertEquals(0,new testUnit().MaxAddArr(Arr3));
		
	}
}
